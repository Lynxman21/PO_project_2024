<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            minHeight="750.0" minWidth="850.0"
            fx:controller="agh.ics.oop.presenter.SimulationViewPresenter">
    <top>
        <Label fx:id="infoLabel" text="Simulation running..." textAlignment="CENTER"/>
    </top>
    <center>
        <VBox>
            <GridPane fx:id="mapGrid" gridLinesVisible="true" prefWidth="800" prefHeight="800" />


        </VBox>
    </center>
</BorderPane>
